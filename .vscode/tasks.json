{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/src/StandardDot.sln"
			],
			"problemMatcher": "$msCompile",
			"group": "build"
		},
		{
			"label": "test",
			"command": "dotnet",
			"type": "process",
			"args": [
				"test",
				"${workspaceFolder}/src/StandardDot.sln"
			],
			"problemMatcher": "$msCompile",
			"group": "build"
		},
		{
			"label": "Start Complete Build",
			"command": "powershell",
			"type": "process",
			"args": [
				"${workspaceFolder}/startCompleteBuild.ps1"
			],
			"problemMatcher": "$msCompile",
			"group": "build"
		}
	]
}